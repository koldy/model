{
	"env": {
		"es6": true,
		"browser": true,
		"jest": true
	},
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"extends": [
		"prettier"
	],
	"plugins": [
		"import"
	],
	"rules": {
		"comma-dangle": ["error", "never"],
		"func-names": 0,
		"no-console": "warn",
		"import/prefer-default-export": 1
	}
}
